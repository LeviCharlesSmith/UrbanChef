<footer class="site-footer">
  <ul class="footer-info">
    <% if current_client %>
      <li class="li_left">Signed in as Client: <%= current_client.email %> </li>
      <li><span class="pipes">|</span></li>
      <li class="li_right"><%= link_to 'Client Log Out', destroy_client_session_path, method: :delete %></li>
    <% elsif current_chef %>
      <li class="li_left">Signed in as Chef: <%= current_chef.email %></li>
      <li><span class="pipes">|</span></li>
      <li class="li_right"><%= link_to 'Chef Log Out', destroy_chef_session_path, method: :delete %></li>
      <% end %>
    </ul>
  </footer>
